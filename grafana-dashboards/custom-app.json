{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    {
      "datasource": "Prometheus",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "targets": [
        {
          "expr": "sum(rate(custom_app_requests_total[5m]))",
          "legendFormat": "rps"
        }
      ],
      "title": "Request Rate (RPS)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "targets": [
        {
          "expr": "sum(rate(custom_app_request_errors_total[5m]))",
          "legendFormat": "errors/s"
        }
      ],
      "title": "Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(custom_app_request_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "title": "P95 Latency (s)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 4,
      "targets": [
        {
          "expr": "sum(custom_app_inflight_requests)",
          "legendFormat": "inflight"
        }
      ],
      "title": "Inflight Requests",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["custom", "app"],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"},
  "timezone": "browser",
  "title": "Custom Metrics App",
  "uid": "custom-metrics-app",
  "version": 1
}
